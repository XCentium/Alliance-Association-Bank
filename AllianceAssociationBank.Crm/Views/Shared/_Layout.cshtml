<!DOCTYPE html>
<html>
<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>@ViewBag.Title</title>
    @Styles.Render("~/Content/css")

</head>
<body>
    @Html.Partial(SharedView.TopNavigationPartial)
    <div class="container body-content">
        @RenderBody()
    </div>
    @* Top Alert Meesage *@
    <div class="alert-fixed-top alert alert-dismissible" id="top-alert" role="alert" style="display: none;">
        <button type="button" class="close js-btn-hide-alert" data-hide-target="#top-alert" aria-label="Close">
            <span aria-hidden="true">&times;</span>
        </button>
        <div class="text-center js-alert-text">
        </div>
    </div>

    @Scripts.Render("~/bundles/jquery")
    @Scripts.Render("~/bundles/popper")
    @Scripts.Render("~/bundles/bootstrap")
    @RenderSection("scripts", required: false)
    @Scripts.Render("~/bundles/jqueryui")
    @Scripts.Render("~/bundles/select2")
    @Scripts.Render("~/bundles/app")

</body>
</html>
